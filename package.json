    "@tanem/react-nprogress": "^4.0.13",
    "axios": "^0.26.1",
    "date-and-time": "^2.4.3",
    "emailjs-com": "^3.2.0",
    "eslint-config-google": "^0.14.0",
    "font-awesome": "^4.7.0",
    "next": "12.0.7",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-lottie": "^1.2.3",
    "react-reveal": "^1.2.2",
